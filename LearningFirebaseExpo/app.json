    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.talha_sheikh.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
